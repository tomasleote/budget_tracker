{
  "name": "budget-tracker-backend",
  "version": "1.0.0",
  "description": "Node.js backend for Budget Tracker application with Supabase integration",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:categories": "jest tests/integration/categories.test.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "migration:create": "node scripts/create-migration.js",
    "migration:run": "node scripts/run-migrations.js",
    "seed": "node scripts/seed-database.js",
    "populate": "node scripts/populate-database.js",
    "data": "ts-node src/scripts/data-cli.ts",
    "data:populate-1m": "ts-node -e \"import('./src/scripts/index').then(m => m.populate1Month())\"",
    "data:populate-3m": "ts-node -e \"import('./src/scripts/index').then(m => m.populate3Months())\"",
    "data:populate-6m": "ts-node -e \"import('./src/scripts/index').then(m => m.populate6Months())\"",
    "data:populate-12m": "ts-node -e \"import('./src/scripts/index').then(m => m.populate12Months())\"",
    "data:delete": "ts-node -e \"import('./src/scripts/index').then(m => m.deleteAllData())\"",
    "data:reset": "ts-node -e \"import('./src/scripts/index').then(m => m.resetDatabase())\"",
    "data:summary": "ts-node -e \"import('./src/scripts/index').then(m => m.getDatabaseSummary()).then(s => console.log(JSON.stringify(s, null, 2)))\"",
    "data:verify": "ts-node -e \"import('./src/scripts/index').then(m => m.verifyDataIntegrity()).then(v => console.log('Valid:', v))\"",
    "optimize:check-env": "ts-node optimization/check-env.ts",
    "optimize:test-performance": "ts-node optimization/test-performance.ts",
    "optimize:apply-indexes": "ts-node optimization/apply-indexes.ts",
    "optimize:apply-repositories": "ts-node optimization/apply-repository-optimizations.ts"
  },
  "keywords": [
    "budget-tracker",
    "finance",
    "typescript",
    "express",
    "supabase",
    "postgresql"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "papaparse": "^5.4.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.0",
    "@types/papaparse": "^5.3.14",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
